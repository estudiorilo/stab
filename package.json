{
  "name": "@estudiorilo/stab",
  "version": "0.1.4",
  "private": false,
  "cdn": "dist/stab.umd.js",
  "main": "dist/stab.cjs.js",
  "unpkg": "dist/stab.umd.js",
  "module": "dist/stab.esm.js",
  "jsdelivr": "dist/stab.umd.js",
  "umd:main": "dist/stab.umd.js",
  "scripts": {
    "build": "bili"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "bili": "^4.8.0",
    "node-sass": "^4.9.0",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/estudiorilo/stab.git"
  },
  "bugs": {
    "url": "https://github.com/estudiorilo/stab/issues"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "bili": {
    "input": "src/index.js",
    "output": {
      "target": "browser",
      "moduleName": "stab",
      "fileName": "stab[min].[format].js",
      "format": ["esm", "cjs", "umd", "umd-min"]
    },
    "plugins": {
      "vue": {
        "css": false
      }
    },
    "babel": {
      "minimal": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "Caio Kawasaki <caio@rilo.com.br>"
}
